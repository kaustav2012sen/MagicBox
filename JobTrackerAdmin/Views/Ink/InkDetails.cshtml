@model IEnumerable<JobTrackerAdmin.Models.Ink>

@{
    ViewBag.Title = "InkDetails";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>InkDetails</h2>

<p>
    @Html.ActionLink("Create New", "InkAddEdit/0")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.InkType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InkRemarks)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.InkType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InkRemarks)
        </td>
        <td>
            @Html.ActionLink("Edit", "InkAddEdit", new { id=item.InkID }) |
            @Html.ActionLink("Details", "Details", new { id=item.InkID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.InkID })
        </td>
    </tr>
}

</table>
